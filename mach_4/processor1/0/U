FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    cone
    {
        type            nonEqMaxwellSlipU;
        accommodationCoeff 1;
        Uwall           ( 0 0 0 );
        refValue        uniform ( 0 0 0 );
        valueFraction   uniform 1;
        thermalCreep    false;
        curvature       false;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           uniform (978.58 0 0);
    }
    rWedge
    {
        type            wedge;
    }
    lWedge
    {
        type            wedge;
    }
    outlet
    {
        type            zeroGradient;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


