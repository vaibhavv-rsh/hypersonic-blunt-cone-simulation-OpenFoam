FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.9e-05";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    cone
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    rWedge
    {
        type            wedge;
    }
    lWedge
    {
        type            wedge;
    }
    outlet
    {
        type            zeroGradient;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 1;
    }
}


